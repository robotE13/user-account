{
    "name": "robote13/user-account",
    "description": "User accounts for web applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Dudal",
            "email": "wolfstrace@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4 | ^8",
        "webmozart/assert": "^1.7.0",
        "ramsey/uuid": "^4.0.1",
        "samdark/hydrator": "^1.0.4",
        "league/container": "^3.3.4",
        "league/tactician": "^1.0.3",
        "robote13/ddd-common": "^0.2.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/verify": "^1.3.0",
        "codeception/specify": "^1.3.0",
        "league/tactician-container": "^2.0"
    },
    "scripts": {
        "codecept": [
            "codecept"
        ],
        "test": [
            "codecept -v run"
        ]
    },
    "autoload": {
        "psr-4": {
            "RobotE13\\UserAccount\\": "src"
        },
        "files": ["src/functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "RobotE13\\UserAccount\\Tests\\": "tests/_support"
        }
    }
}
